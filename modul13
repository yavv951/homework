
c=int(input("введите кол-во билетов: "))
L = {a:int(input("введите возраст посетителя: ")) for a in range(1, c+1)} #тут я создал словарь и каждому посетителю присвоил возраст
print("--------------")
print(L)
my_new_list=list (map (int,L.values())) #тут я создал лист с возрастом каждого посетителя
print(my_new_list )
q=0 #счетчик чей возраст от 18 до 25 лет
f=0 #счетчик чей возраст от 25 лет
n=0 #счетчик чей возраст до 18 лет
for i in my_new_list:
    if 18 <= i <= 25:
        q=q+990
    elif i  > 25 :
        f=f+1390
    else:
        n += 1
p=q+f
print("вход бесплатный для " + str(n) + " посетителей")
print('Общая стоимость билетов без скидки:' + str(p))
if c > 3:
    d=p*0.9
    print('Общая стоимость билетов со скидкой 10%:' + str(d))
else:
    print("Скидка 10 % не предоставляется, так как посетителей меньше 3 человек")
